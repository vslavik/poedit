test-suite boost-geometry-algorithms-relational
    :
    [ run covered_by.cpp ]
    [ run crosses.cpp ]
    [ run equals.cpp ]
    [ run intersects.cpp ]
    [ run multi_covered_by.cpp ]
    [ run multi_equals.cpp ]
    [ run multi_intersects.cpp ]
    [ run multi_touches.cpp ]
    [ run overlaps.cpp ]
    [ run touches.cpp ]
    ;

build-project disjoint ;
build-project relate ;
build-project within ;
